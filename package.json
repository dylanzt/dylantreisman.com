{
  "name": "dylantreisman.com",
  "version": "1.0.0",
  "description": "Personal website of Dylan Treisman",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker": "docker-compose up -d --build",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --ext .js --ext .jsx ./src ./config ./webpack",
    "eslint:fix": "eslint --ext .js --ext .jsx --fix ./src ./config ./webpack",
    "stylelint": "stylelint './src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint --fix './src/**/*.{css,scss}'",
    "dev": "webpack serve --open --history-api-fallback --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylanzt/dylantreisman.com.git"
  },
  "author": "Dylan Treisman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dylanzt/dylantreisman.com/issues"
  },
  "homepage": "https://github.com/dylanzt/dylantreisman.com#readme",
  "dependencies": {
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "pino": "^6.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.1.1",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.1",
    "html-webpack-plugin": "^5.2.0",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^9.0.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
